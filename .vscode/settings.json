{
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": "MAZE-WORLD/.*.js",
        "cmd": "node MAZE-WORLD/scripts/minify-js.js"
      },
      {
        "match": "MAZE-WORLD/.*.css",
        "cmd": "node MAZE-WORLD/scripts/minify-css.js"
      },
      {
        "match": "JOKENPO/.*.js",
        "cmd": "node JOKENPO/scripts/minify-js.js"
      },
      {
        "match": "JOKENPO/.*.css",
        "cmd": "node JOKENPO/scripts/minify-css.js"
      }
    ]
  }
}
